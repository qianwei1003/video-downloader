{
  "downloadPath": "downloads",
  "autoStartServer": true,
  "defaultQuality": "highest",
  "language": "zh-CN",
  "batchDownload": {
    "enabled": true,
    "concurrentLimit": 3,
    "retryCount": 3,
    "retryDelay": 2000,
    "autoRetryFailed": true,
    "outputTemplate": "${title}-${quality}"
  },
  "platforms": {
    "youtube": {
      "enabled": true,
      "options": {
        "maxRetries": 3,
        "retryDelay": 2000
      }
    },
    "bilibili": {
      "enabled": true,
      "options": {
        "maxRetries": 3,
        "retryDelay": 2000,
        "userAgent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36",
        "cookies": ""
      }
    }
  }
}
